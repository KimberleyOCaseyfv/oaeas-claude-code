# OAEAS 免费模式配置
# 所有报告默认完全开放，无需支付

# 模式设置
MODE=free  # free 或 paid

# 免费模式特有配置
FREE_MODE_ALL_REPORTS_DEEP=true  # 所有报告都是深度报告
FREE_MODE_SHOW_CTA=true  # 显示"再次测评"CTA

